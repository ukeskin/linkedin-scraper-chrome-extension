{
  "manifest_version": 3,
  "name": "LinkedIn Job Scraper",
  "version": "1.0",
  "description": "Scrapes job listings from LinkedIn and exports to CSV",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon48.png"
  }
}
